    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.4.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.1.2",
    "jasmine": "^2.7.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-rollup-preprocessor": "^4.0.3",
    "prettier": "^1.5.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.47.4",
